<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleHead">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
        <attr name="radius" format="dimension" />
        <attr name="switchOnColor" format="color" />
        <attr name="switchOffColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="isSwitchOn" format="boolean"/>
        <attr name="angle" format="integer" />
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1"/>
            <enum name="round" value="2"/>
        </attr>
        <attr name="radiusround" format="dimension"/>
    </declare-styleable>
</resources>